{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "footer-layout.mobile": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "flex-layout.row#footer": { 
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "paddingTop":1,
      "paddingBottom": 2,
      "marginTop": 1,
      "blockClass": "footerContainer"
    },
    "children": [
      "flex-layout.col#login-footer",
      "whats"
    ]
  },
  "flex-layout.col#login-footer": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [ "modal-trigger#cadastro"]
  },
  "modal-trigger#cadastro": {
    "children": [
      "image#cadastro",
      "rich-text#cadastro",
      "modal-layout#cadastro"
    ],
    "props": {
      "trigger": "click"
    }
  },
  "image#cadastro": {
    "props": {
      "src": "https://cdn.statically.io/gh/sergiofdf/images/main/login.svg",
      "maxHeight": 30
    }
  },
  "rich-text#cadastro": {
    "props": {
      "text": "CADASTRO"
    }
  },
  "modal-layout#cadastro": {
    "children": [
      "modal-content"
    ]
  },
  "modal-content":{
    "children": [
      "userform"
    ]
  }
}